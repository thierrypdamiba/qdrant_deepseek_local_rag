# Qdrant Cloud Configuration
QDRANT_URL=https://your-qdrant-instance.region.cloud.qdrant.io

# Role-specific API Keys
HEAD_OF_SUPPORT_API_KEY=your_head_of_support_api_key_here
ACCOUNT_MANAGER_A_API_KEY=your_account_manager_a_api_key_here
ACCOUNT_MANAGER_B_API_KEY=your_account_manager_b_api_key_here
ACCOUNT_MANAGER_C_API_KEY=your_account_manager_c_api_key_here
SUPPORT_AGENT_API_KEY=your_support_agent_api_key_here

# Default API Key (using Head of Support's key for full access)
QDRANT_API_KEY=${HEAD_OF_SUPPORT_API_KEY}

# OpenAI Configuration
NEXT_PUBLIC_OPENAI_API_KEY=your_openai_api_key_here
OPENAI_API_KEY=your_openai_api_key_here

# Ollama Configuration
OLLAMA_BASE_URL=http://localhost:11434
OLLAMA_MODEL=deepseek-r1:1.5b

# Next.js Configuration
NEXT_PUBLIC_API_BASE_URL=http://localhost:3000